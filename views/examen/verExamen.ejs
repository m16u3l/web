
<section id="contact" class="fill">
    <div class="container wow fadeInUp">
      <h1>EN DESARROLLO</h1>
      <div class="row">
        <div class="col-md-12">
          <h3 class="section-title">Examenes</h3>
          <div class="section-title-divider"></div>
          <p class="section-description">Pregunta</p>
        </div>
      </div>
      
      <div class="row">
        
        <div class="col-md-8 col-md-push-3">
          <div class="form">
            <div id="sendmessage">Su registro fue enviado</div>
            <div id="errormessage"></div>
            <form action="/evaluador/create" method="post" role="form" class="contactForm">

                <div class="form-group">
                  <label>Pregunta</label>
                  <input type="text" name="id_evaluador" class="form-control" id="name" placeholder="Pregunta" data-rule="minlen:4" data-msg="Ingrese el ID del evaluador" title="Nombre" required/>
                  <div class="validation"></div>
                </div>

                <div class="form-group">
                  <label>Respuesta</label>
                  <input type="text" name="nombre_evaluador" class="form-control" id="name" placeholder="respuestas" data-rule="minlen:5" data-msg="Ingrese el nombre del evaluador" title="Nombre" required/>
                  <div class="validation"></div>
                </div>

               
                <div class="form-group">
                  <label>Area:</label>
                  <select class="form-control" name="area_evaluador">
                      <option value="Quimica">Quimica</option>
                      <option value="Fisica">Fisica</option>
                      <option value="Matematicas">Matematicas</option>
                      <option value="Biologia">Biologia</option>
                 </select>
                  <div class="validation"></div>
                </div>
                
                <div class="text-center">
                  <input type="submit" name="Registrar Evaludor" value="Registrar Preguntas" class="btn btn-default"/>
                </div>
            </form>
          </div>
        </div>
        
      </div>
 </div>
  </section>